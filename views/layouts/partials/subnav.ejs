<div class="subNav">
  <% if(titleObj.includes("Quotes")) {%> 
    <form <% if(mode === "true") {%>  action="/member/home/academic" <% } else { %> action="/lmj/member/home/lay" <% } %>  method="GET" id="myQuote">
      <button class="myQuoteBtn" type="submit" form="myQuote" id="btnMover">ADD QUOTE</button>
      </form>
    <% } else {%>
      <form action="/member/myQuote" method="GET" id="myQuote">
        <button class="myQuoteBtn" type="submit" form="myQuote">MY QUOTES</button>
        </form>
      <% } %>  
    <div class="navProfile" id="navProfile">
      <% if(typeof profileUrl == "string") {%> 
        <img src="<%= profileUrl %>" alt="">
        <% } %> 
    </div>
  </div>
  <div class="profileContainer profileHide" id="profileContainer">
    <div class="textFader" id="textFader">
    <div class="profile">
      <p class="proWelcome">Welcome!</p>
      <div class="navProfile2" id="navProfile2">
        <% if(typeof profileUrl == "string") {%> 
          <img src="<%= profileUrl %>" alt="">
          <% } %> 
      </div>
      <p class="proUser"><%= profileObj %></p>
      <div class="proAcademic">
        <p class="modeLabel">Academic Mode:</p>
        <form action="/member/modulate" method="POST" id="modalForm">
        <label class="modeSwitch">
          <% if(mode === "true") { %> 
          <input type="checkbox" name="mode" value="false" <%= "checked" %> >
          <% } else { %> 
            <input type="checkbox" name="mode" value="true">
            <% } %> 
          <span class="modeSlider modeRound"></span>
        </label>
      </form>
      </div>
    </div>
      <form action="/member/photoUpload" class="uploadForm" id="uploadForm" method="POST" enctype="multipart/form-data">
  <div class="uploadThumb">
        <label class="uploadText" for="myFile">Click <span>here</span> to select profile image:</label><i class="far fa-thumbs-up thumbPos" id="uploadThumb"></i>
      </div>
        <input class="uploadInput" type="file" id="myFile" name="filename"/>
    <div class="uploadDiv">
          <label for="imgUploadBtn" class="proLabel">Upload Image</label>
          <button type="submit" for="uploadForm" id="imgUploadBtn" class="proBtn"><i class="far fa-images"></i></button>
        </div>
      </form>
      <form action="/member/photoDelete?_method=PUT" class="photoDelete" id="photoDelete" method="POST">
        <div class="uploadDiv">
        <label class="proLabel" for="photoDelBtn">Delete Image</label>
        <button type="submit" form="photoDelete" id="photoDelBtn" class="proBtn bin"><i class="far fa-trash-alt"></i></button>
        </div>
      </form>
        <hr>
    <div class="optionsContainer">
      <form action="/member/logout" id="logOut" method="GET">
        <div class="logDiv">
        <label for="logOutBtn" class="proLabel">Log Out</label>
        <button type="submit" form="logOut" id="logOutBtn" class="proBtn"><i class="fas fa-sign-out-alt"></i></button>
        </div>
      </form>
      <form action="/member/deleteAc?_method=DELETE" id="deleteAc" method="POST">
        <div class="logDiv">
        <label for="deleteAcBtn" class="proLabel">Delete Account</label>
        <button type="submit" form="deleteAc" id="deleteAcBtn" class="proBtn bin"><i class="far fa-trash-alt"></i></button>
        </div>
      </form>
    </div>
    </div>
    </div>
  </div>
  <script src="/js/subNav.js" type="text/javascript"></script>
